{
  "extends": "@closetcutie/tsconfig/vue",
  "compilerOptions": {
    "types": ["@dcloudio/types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@closetcutie/types": ["../../packages/types/src"],
      "@closetcutie/types/*": ["../../packages/types/src/*"],
      "@closetcutie/services": ["../../packages/services/src"],
      "@closetcutie/services/*": ["../../packages/services/src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
