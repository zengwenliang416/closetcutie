{
  "extends": "@closetcutie/tsconfig/vue",
  "compilerOptions": {
    "types": ["@dcloudio/types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@types": ["../../packages/types"],
      "@types/*": ["../../packages/types/*"],
      "@services": ["../../packages/services"],
      "@services/*": ["../../packages/services/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
